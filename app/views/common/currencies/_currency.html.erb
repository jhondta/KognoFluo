<div id="<%= dom_id currency %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= currency.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Code alpha:</strong>
    <%= currency.code_alpha %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Code numeric:</strong>
    <%= currency.code_numeric %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Symbol:</strong>
    <%= currency.symbol %>
  </p>

</div>
